{
  "name": "js-starter-kit",
  "version": "1.0.0",
  "description": "JavaScript Development Environment",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node ./buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node ./buildScripts/srcServer.js",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jose-garcia86/js-dev-env.git"
  },
  "keywords": [
    "JavaScript",
    "Starter",
    "Kit",
    "Development",
    "Environment"
  ],
  "author": "Jose Garcia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jose-garcia86/js-dev-env/issues"
  },
  "homepage": "https://github.com/jose-garcia86/js-dev-env#readme",
  "dependencies": {
    "chalk": "^4.1.1",
    "localtunnel": "^2.0.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "compression": "^1.7.4",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.1",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.1",
    "jsdom": "^16.5.3",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "^8.4.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.0",
    "webpack-dev-middleware": "^4.2.0"
  }
}
